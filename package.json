{
	"name": "eleventy-base-blog",
	"version": "9.0.0",
	"description": "A starter repository for a blog web site using the Eleventy site generator.",
	"type": "module",
	"scripts": {
		"start": "npm run dev",
		"dev": "run-p 'dev:*' --print-label",
		"dev:eleventy": "eleventy --serve",
		"dev:vite": "vite",
		"build": "NODE_ENV=production run-s clean build:vite build:eleventy clean:vite --print-label",
		"build:eleventy": "eleventy",
		"build:vite": "vite build",
		"clean": "del-cli dist",
		"clean:vite": "del-cli dist/.vite",
		"debug": "cross-env DEBUG=Eleventy* npm run start",
		"debugstart": "cross-env DEBUG=Eleventy* npx @11ty/eleventy --serve --quiet",
		"benchmark": "cross-env DEBUG=Eleventy:Benchmark* npx @11ty/eleventy"
	},
	"repository": {
		"type": "git",
		"url": "git://github.com/11ty/eleventy-base-blog.git"
	},
	"author": {
		"name": "Zach Leatherman",
		"email": "zachleatherman@gmail.com",
		"url": "https://zachleat.com/"
	},
	"license": "MIT",
	"engines": {
		"node": ">=18"
	},
	"funding": {
		"type": "opencollective",
		"url": "https://opencollective.com/11ty"
	},
	"bugs": {
		"url": "https://github.com/11ty/eleventy-base-blog/issues"
	},
	"homepage": "https://github.com/11ty/eleventy-base-blog#readme",
	"devDependencies": {
		"@11ty/eleventy": "^3.1.0",
		"@11ty/eleventy-img": "^6.0.4",
		"@11ty/eleventy-navigation": "^1.0.4",
		"@11ty/eleventy-plugin-rss": "^2.0.4",
		"@11ty/eleventy-plugin-syntaxhighlight": "^5.0.1",
		"cross-env": "^7.0.3",
		"del-cli": "^6.0.0",
		"luxon": "^3.6.1",
		"npm-run-all": "^4.1.5",
		"prismjs": "^1.30.0",
		"zod": "^3.24.4",
		"zod-validation-error": "^3.4.1"
	},
	"dependencies": {
		"@zachleat/heading-anchors": "^1.0.3",
		"eleventy-plugin-page-assets": "^0.3.0",
		"eleventy-plugin-svg-sprite": "^2.4.4",
		"html-minifier": "^4.0.0",
		"vite": "^6.3.5"
	}
}
